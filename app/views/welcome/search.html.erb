<h2>Search Results for <%= params[:search].split.map(&:capitalize).join(' ')%></h2>

  <%= form_tag '/places', method: :post do %>
    <%= hidden_field_tag(:search, params[:search]) %>
    <%= submit_tag 'Save to Favorites', data: { confirm: "Saved to Favorite Places! Go to Profile so see favorites"}, :class => "favoriteBtn" %>

  <% end %>

<p>
<% @photos.each do |pic| %>
  <!-- <a href="https://farm<%= pic["farm"].to_s %>.staticflickr.com/<%= pic["server"] %>/<%= pic["id"] %>_<%= pic["secret"] %>_z.jpg"><img src="https://farm<%= pic["farm"].to_s %>.staticflickr.com/<%= pic["server"] %>/<%= pic["id"] %>_<%= pic["secret"] %>_m.jpg"></a> -->
  <a class="photoSearch" href="https://farm<%= pic["farm"].to_s %>.staticflickr.com/<%= pic["server"] %>/<%= pic["id"] %>_<%= pic["secret"] %>_z.jpg" style="width:120px; height:90px; display:inline-block; background-size:cover; background-postion:center center; background-image:url('https://farm<%= pic["farm"].to_s %>.staticflickr.com/<%= pic["server"] %>/<%= pic["id"] %>_<%= pic["secret"] %>_m.jpg');"></a>
<% end %>
</p>
